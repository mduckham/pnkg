<#OfficialPlaceNameMap> a rr:TriplesMap;

  rml:logicalSource [
    rml:source "/Users/alexis/Documents/04_Code/semadaten/data/SA/SA_sample_Gazetteer_geojson/SA_sample_GazetteerSites_GDA94.geojson";
    rml:referenceFormulation ql:JSONPath;
    rml:iterator "$.features[*]"
    ];

    rr:subjectMap [
        rr:template "http://linked.data.gov.au/def/placenames/{properties.recno}";
    ];

    rr:predicateObjectMap [
        rr:predicate rdfs:member;
        rr:objectMap [ 
            rml:reference "properties.recno" 
        ] 
    ];

    rr:predicateObjectMap [
        rr:predicate loci:isMemberOf;
        rr:objectMap [ 
            rml:reference "" 
        ] 
    ];

    rr:predicateObjectMap [
        rr:predicate :status;
        rr:objectMap [ 
            rml:reference ":official"
        ] 
    ];

    rr:predicateObjectMap [
        rr:predicate :wasNamedBy;
        rr:objectMap [ 
            rml:reference "properties.lga" 
        ] 
    ];

    rr:predicateObjectMap [
        rr:predicate :dateGazetted;
        rr:objectMap [ 
            rml:reference "properties.gazdate" 
        ] 
    ];

    rr:predicateObjectMap [
        rr:predicate :datePublished;
        rr:objectMap [ 
            rml:reference "properties.date_cr_al" 
        ] 
    ];

    rr:predicateObjectMap [
        rr:predicate :dateReleased;
        rr:objectMap [ 
            rml:reference "" 
        ] 
    ];

    rr:predicateObjectMap [
        rr:predicate :dateRetired;
        rr:objectMap [ 
            rml:reference "" 
        ] 
    ];

    rr:predicateObjectMap [
        rr:predicate dcterms:identifier;
        rr:objectMap [ 
            rml:reference "" 
        ] 
    ].

